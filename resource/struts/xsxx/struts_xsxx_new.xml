            <forward name="view" path="/xsgzgl/xsxx/xxxg/viewgrhjfj.jsp"></forward><!-- 北京中医药个人附件上传 -->
        </action>

        <action path="/xsxx_xsxxxgsh"
                scope="request"
                type="com.zfsoft.xgxt.xsxx.xsxxgl.shlc.ShlcAction"
                name="shlcModel"
                parameter="method">
