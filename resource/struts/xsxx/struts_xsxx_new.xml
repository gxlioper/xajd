            <forward name="getXscj" path="/xsgzgl/xsxx/xscjcx/xscj.jsp"></forward>
        </action>

        <!--书院学院对应-->
        <action path="/xsxx_xsxxgl_syxydy" forward="/syxydy.do?method=syXyList"></action>
        <action path="/syxydy"
                scope="request"
                type="com.zfsoft.xgxt.xsxx.xsxxgl.syxydy.SyxyAction"
                name="syxyForm"
                parameter="method">
