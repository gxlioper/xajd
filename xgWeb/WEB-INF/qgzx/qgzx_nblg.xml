<struts-config>
	<form-beans>
    <form-bean name="commanForm" type="xgxt.form.CommanForm" />
	</form-beans>

	<global-forwards />

	<action-mappings>
    <action 
    	path="/qgzxNblg" 
    	type="xgxt.qgzx.nblg.QgzxNblgAction"
    	parameter="method"    	
    	name="commanForm"
    	scope="request">
    	<forward name="xsgwsqb" path="/qgzx/nblg/xsgwsqb.jsp"/>    
    	<forward name="gwsqb" path="/qgzx/nblg/gwsqb.jsp"/> 	   	  	    	
    </action>
		
	</action-mappings>
</struts-config>

