		<action path="/wjcf_nblg_cxprint"      forward="/wjcfnblgwh.do?method=cxPrint"></action>  <!-- 撤消处分报表打印 -->
		<action path="/wjcf_nblg_wyhjdwh"      forward="/wjcfnblgwh.do?method=wyhjdWh"></action><!-- 委员会决定维护页面 -->
		<action path="/wjcf_nblg_cfjdUpdate"   forward="/wjcfnblgwh.do?method=cfjdUpdate"></action><!-- 委员会决定 -->
		<action path="/wjcf_nblg_cdtyWjcx"    forward="/wjcfnblgwh.do?method=cdtyWjcx"></action><!-- 委员会决定 -->
	</action-mappings>
</struts-config>
