<%@ page language="java" contentType="text/html; charset=GBK"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" 
"http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<html xmlns="http://www.w3.org/1999/xhtml">
	<head>
		<%@ include file="/syscommon/head.ini"%>
		<script type="text/javascript" src="js/jquery/plugins/dataGrid/dataGrid.js"></script>
		<script type="text/javascript" src="js/calendar/calendar.js"></script>
		<script type="text/javascript" src="xsgzgl/szdw/jtff/jtff/js/jtff.js"></script>
		<script type="text/javascript">
		jQuery(function(){
			var gridSetting = {
				caption : "津贴发放信息列表",
				pager : "pager",
				url : "jtff_jtff.do?method=getJtffcx&type=query",
				colList : [ 
							{label:'key',name:'id', index: 'id',key:true ,hidden:true},
							{label:'职工号',name:'zgh', index: 'zgh',width:'8%',formatter:xhLink},
							{label:'姓名',name:'xm', index: 'xm',width:'6%'},
							{label:'性别',name:'xb', index: 'xb',width:'6%'},
							{label:'所属部门',name:'bmmc', index: 'bmmc',width:'10%'},
							{label:'岗位',name:'gw', index: 'gw',width:'10%'},
							{label:'所带学生数',name:'dbrs', index: 'dbrs',width:'6%'},
							{label:'发放月份',name:'ffny', index: 'ffny',width:'6%'},
							{label:'发放金额(元)',name:'ffje', index: 'ffje',width:'6%'}
							]
			}
			var map = getSuperSearch();
			gridSetting["params"] = map;
			jQuery("#dataTable").initGrid(gridSetting);
		})
		</script>
	</head>

	<body>
		<input type="hidden" name="isopen" id="isopen" value="${jcszModel.isopen }"/>
		<div class="tab_cur">
			<p class="location">
				<em>您的当前位置：</em><a>${title }</a>
			</p>
		</div>
		<html:form action="/jtff_jtff">
			<%@ include file="/comm/hiddenValue.jsp"%>
			
			<div class="toolbox">
				<!-- 按钮 -->
				<div class="buttonbox">
					<ul>
						<logic:equal name="writeAble" value="yes">
						<li>
							<a href="javascript:void(0);" class="btn_zj" onclick="scjt();return false;"  >生成津贴</a>
						</li>
						</logic:equal>
						<li><a href="#" class="btn_dc" onclick="exportConfig();return false;">导出</a></li>	
					</ul>
				</div>
				<!-- 过滤条件 -->	
				<%@ include file="/comm/search/superSearchArea.jsp"%>
				<!-- 过滤条件 end-->
			</div>
		</html:form>
		
		<div class="main_box">
			<h3 class=datetitle_01>
				<span>津贴发放信息列表&nbsp;&nbsp; </span>
			</h3>
			<div class="con_overlfow">
				<table id="dataTable" ></table>
				<div id="pager"></div>
			</div>
		</div>
	</body>
</html>
